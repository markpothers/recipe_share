{
	"expo": {
		"name": "Recipe-Share",
		"slug": "recipe-share",
		"privacy": "public",
		"platforms": [
			"ios",
			"android"
		],
		"version": "0.0.001",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.recipe_share.recipe_share"
		}
	}
}
